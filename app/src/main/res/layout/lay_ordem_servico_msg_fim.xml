<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="45dp"
            android:layout_gravity="center"
            android:layout_marginTop="10dp"
            android:text="Atenção"
            android:textColor="@color/legacy_selected_highlight"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/lblOsFimTexto1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:text="Encerramento da OS 999999"
            android:textColor="@color/legacy_selected_highlight"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/lblOsFimTexto2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="A Ordem de Serviço 99999 foi encerrada com sucesso."
            android:textColor="@color/legacy_selected_highlight"
            android:textSize="18sp" />


        <Button
            android:id="@+id/btnOSFim"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="OK" />

    </LinearLayout>
</ScrollView>

